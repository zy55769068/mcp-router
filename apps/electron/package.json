{
  "name": "@mcp_router/electron",
  "productName": "MCP Router",
  "version": "0.5.4",
  "private": true,
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-router/mcp-router.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start --enable-logging",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "knip": "cd ../.. && knip --workspace apps/electron",
    "postinstall": "electron-rebuild",
    "rebuild": "electron-rebuild",
    "test:e2e": "pnpm make && cp package.json .webpack/arm64/main/ && playwright test --config=e2e/playwright-electron.config.ts",
    "test:e2e:only": "playwright test --config=e2e/playwright-electron.config.ts",
    "test:e2e:headed": "pnpm make && cp package.json .webpack/arm64/main/ && playwright test --config=e2e/playwright-electron.config.ts --headed"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.12",
    "@anthropic-ai/dxt": "^0.2.6",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mcp_router/remote-api-types": "workspace:*",
    "@mcp_router/shared": "workspace:*",
    "@mcp_router/tailwind-config": "workspace:*",
    "@mcp_router/ui": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@tabler/icons-react": "^3.34.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@uiw/react-codemirror": "^4.24.2",
    "ai": "^4.3.16",
    "better-sqlite3": "^11.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "electron-squirrel-startup": "^1.0.1",
    "execa": "^9.6.0",
    "express": "^5.0.1",
    "highlight.js": "^11.11.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.487.0",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12",
    "posthog-js": "^1.258.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.2",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "semver": "^7.7.2",
